{
    "user": "ziggi",
    "repo": "rustext",
    "resources": [
        {
            "name": "^rustext-(.+).zip$",
            "platform": "linux",
            "archive": true,
            "includes": ["rustext.inc"],
            "plugins": ["rustext.so"]
        },
        {
            "name": "^rustext-(.+).zip$",
            "platform": "windows",
            "archive": true,
            "includes": ["rustext.inc"],
            "plugins": ["rustext.dll"]
        }
    ],
    "runtime": {
        "plugins": ["ziggi/rustext"]
    }
}